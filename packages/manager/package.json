{
  "name": "@outclimb/manager",
  "version": "1.0.0",
  "description": "OutClimb Management Frontend",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OutClimb/OutClimb.git"
  },
  "author": "Nichole Mattera",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OutClimb/OutClimb/issues"
  },
  "homepage": "https://github.com/OutClimb/OutClimb#readme",
  "scripts": {
    "build": "tsc -b && vite build",
    "build:watch": "tsc -b && vite build --watch --mode dev",
    "dev": "vite",
    "format": "prettier ./src",
    "format:fix": "prettier --write ./src",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fontsource/poppins": "^5.2.7",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-router": "^1.159.5",
    "@tanstack/react-router-devtools": "^1.159.4",
    "@tanstack/react-table": "^8.21.3",
    "@zip.js/zip.js": "^2.6.40",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.562.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.4",
    "shadcn": "^3.7.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "zustand": "^5.0.11"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "arrowParens": "always"
  }
}
